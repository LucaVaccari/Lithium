<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root maxWidth="1.7976931348623157E308" prefHeight="0.0" prefWidth="900.0" type="AnchorPane"
         xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="0.0" prefWidth="900.0" spacing="5.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Button fx:id="backBtn" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                mnemonicParsing="false" prefHeight="0.0" prefWidth="41.0" text="Back" HBox.hgrow="ALWAYS"/>
        <Button fx:id="pauseBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0"
                maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="0.0" prefWidth="47.0"
                text="Pause" HBox.hgrow="ALWAYS"/>
        <Button fx:id="forwardBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="51.0" layoutY="10.0"
                maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefHeight="0.0" prefWidth="60.0"
                text="Forward" HBox.hgrow="ALWAYS"/>
        <Slider fx:id="progressSlider" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                prefHeight="0.0" prefWidth="393.0" HBox.hgrow="ALWAYS"/>
        <Label fx:id="progressLbl" prefHeight="0.0" prefWidth="29.0" text="00:00"/>
    </HBox>
</fx:root>
