<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root prefHeight="40.0" prefWidth="150.0" type="AnchorPane" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <ImageView fx:id="userProPicImg" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true"
                   HBox.hgrow="ALWAYS"/>
        <VBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
            <Label fx:id="usernameLbl" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   text="Username" VBox.vgrow="ALWAYS"/>
            <Label fx:id="roleLbl" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                   maxWidth="1.7976931348623157E308" text="Role" VBox.vgrow="ALWAYS">
                <font>
                    <Font name="System Italic" size="11.0"/>
                </font>
            </Label>
        </VBox>
    </HBox>
</fx:root>
