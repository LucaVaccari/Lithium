<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="40.0" prefWidth="110.0" type="AnchorPane" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <ImageView fx:id="userProPicImg" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                   preserveRatio="true" HBox.hgrow="ALWAYS"/>
        <Label fx:id="usernameLbl" text="Username" HBox.hgrow="ALWAYS"/>
    </HBox>
</fx:root>
